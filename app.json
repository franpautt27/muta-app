{
  "expo": {
    "name": "muta-app",
    "slug": "muta-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/muta-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/muta-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1c1c1c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/muta-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
